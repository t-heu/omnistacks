{
  "expo": {
    "name": "mobile",
    "slug": "mobile",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "androidStatusBarColor": "#334393",
    "androidStatusBar": {
      "barStyle": "light-content",
      "backgroundColor": "#334393"
    },
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "ca.brentvatne.growlerprowler"
    },
    "android": {
      "package": "ca.brentvatne.growlerprowler",
      "config": {
        "googleMaps": {
          "apiKey": "API KEY"
        }
      }
    }
  }
}
